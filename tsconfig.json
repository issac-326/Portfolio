{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "src/components/*",
      ],
      "@layouts-components/*": ["src/layouts/components/*"],
      "@pages-components/*": ["src/pages/components/*"],
    }
  }
}